    "authentication"
  ],
  "author": "DannyWhyte",
  "license": "MIT",
  "main": "app.js",
  "dependencies": {
    "chai": "^4.1.2",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.12.1",
    "jsonschema": "^1.2.4",
    "mocha": "^5.2.0",
    "moment": "^2.29.4",
    "pg": "^7.17.1",
    "q": "^1.5.1",
    "standard": "^14.3.1",
    "swagger-ui-express": "^3.0.9",
    "underscore": "^1.9.0"
  },
  "devDependencies": {
    "should": "^7.1.0",
    "supertest": "^1.0.0"
